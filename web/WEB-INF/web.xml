<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
  <servlet>
      <servlet-name>AutorizationController</servlet-name>
      <servlet-class>phonestation.view.AutorizationController</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>AutorizationController</servlet-name>
      <url-pattern>/AutorizationController</url-pattern>
  </servlet-mapping>
  <servlet>
      <servlet-name>RegistrationController</servlet-name>
      <servlet-class>phonestation.view.RegistrationController</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>RegistrationController</servlet-name>
      <url-pattern>/RegistrationController</url-pattern>
  </servlet-mapping>
    <servlet>
        <servlet-name>AccountController</servlet-name>
        <servlet-class>phonestation.view.AccountController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AccountController</servlet-name>
        <url-pattern>/AccountController</url-pattern>
    </servlet-mapping>
</web-app>